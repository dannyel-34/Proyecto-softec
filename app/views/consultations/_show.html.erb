<div class='Consulta'>
<div class="panel panel-default">
<div class="panel-body">
<p id="notice"><%= notice %></p>

<p>
  <strong>Alumno:</strong>
  <%= @consultation.alumno.nombre_completo %>
</p>

<p>
  <strong>Curso:</strong>
  <%= @consultation.curso.nombre %>
</p>

<p>
  <strong>Grupo:</strong>
  <%= @consultation.grupo.nombre %>
</p>

<p>
  <strong>Horario:</strong>
  <%= @consultation.horario.asiste %>
</p>

<p>
  <strong>Institucion:</strong>
  <%= @consultation.institucion.cod %>
</p>

<p>
  <strong>Sucursal:</strong>
  <%= @consultation.sucursal.cod %>
</p>

<p>
  <strong>Profesor:</strong>
  <%= @consultation.profesor.nombre_completo %>
</p>


<p>
  <strong>User:</strong>
  <%= @consultation.user.name %>
</p>

<center>
<%= link_to 'Editar', edit_consultation_path(@consultation), class: "btn btn-warning btn-sm" %> |
<%= link_to 'Atras', consultations_path, class: "btn btn-success btn-sm" %>
</center>
</div></div></div>